{
  "version": 1,
  "projects": {
    "components": {
      "projectType": "library",
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "prefix": "zyfra",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/components"],
          "options": {
            "project": "libs/components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/components"],
          "options": {
            "jestConfig": "libs/components/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/components/src/**/*.ts", "libs/components/src/**/*.html"]
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/components/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/components",
            "config": {
              "configFolder": "libs/components/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "doc": {
          "builder": "./tools/executors/compodoc:compodoc",
          "options": {
            "tsConfigPath": "./libs/components/tsconfig.json",
            "distPath": "./libs/components"
          }
        },
        "build-icons": {
          "builder": "./tools/executors/icon-font-builder:build"
        },
        "build-icons-16": {
          "builder": "./tools/executors/icon-font-builder:build",
          "options": {
            "fontFileName": "ZyfraIcons16",
            "fontName": "Zyfra Icons 16",
            "iconClassName": ".zyfra-icon-16",
            "inputPath": "./libs/components/assets/icons-16",
            "outputPath": "./libs/components/src/styles/icons-16",
            "outputStyleFile": "icons-16.less",
            "outputDefinitionsTs": "./libs/components/src/lib/icon/story/icon-16-definitions.ts",
            "outputDefinitionsVar": "Icon16Defs"
          }
        }
      },
      "tags": []
    },
    "components-e2e": {
      "root": "apps/components-e2e",
      "sourceRoot": "apps/components-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/components-e2e/cypress.json",
            "devServerTarget": "components:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "components:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/components-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["components"]
    },
    "doc": {
      "projectType": "application",
      "root": "apps/doc",
      "sourceRoot": "apps/doc/src",
      "prefix": "zyfra",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "customWebpackConfig": {
              "path": "apps/doc/webpack.config.ts"
            },
            "outputPath": "dist/apps/doc",
            "index": "apps/doc/src/index.html",
            "main": "apps/doc/src/main.ts",
            "polyfills": "apps/doc/src/polyfills.ts",
            "tsConfig": "apps/doc/tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "apps/doc/src/favicon.ico",
              "apps/doc/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/@taiga-ui/icons/src",
                "output": "assets/taiga-ui/icons"
              },
              {
                "glob": "**/*",
                "input": "libs/components/src/styles/fonts",
                "output": "assets/ui-platform/components/fonts"
              }
            ],
            "styles": [
              "apps/doc/src/styles.less",
              "node_modules/@taiga-ui/core/styles/taiga-ui-global.less",
              "node_modules/@taiga-ui/core/styles/taiga-ui-theme.less",
              "node_modules/primeicons/primeicons.css",
              "libs/next/src/styles/styles.less",
              "libs/next/src/styles/icons/icons.less",
              "libs/next/src/styles/icons-16/icons-16.less",
              "node_modules/prismjs/themes/prism-okaidia.css",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.css"
            ],
            "scripts": [
              "node_modules/marked/lib/marked.js",
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/components/prism-less.min.js",
              "node_modules/prismjs/components/prism-markup.min.js",
              "node_modules/prismjs/components/prism-typescript.min.js",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.js",
              "node_modules/clipboard/dist/clipboard.min.js"
            ]
          },
          "configurations": {
            "production": {
              "deployUrl": "/pzm-sdk-new/doc/",
              "baseHref": "/pzm-sdk-new/doc/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb",
                  "maximumError": "100kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/doc/src/environments/environment.ts",
                  "with": "apps/doc/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "doc:build:production"
            },
            "development": {
              "browserTarget": "doc:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "doc:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/doc/src/**/*.ts", "apps/doc/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/doc"],
          "options": {
            "jestConfig": "apps/doc/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "doc-e2e": {
      "root": "apps/doc-e2e",
      "sourceRoot": "apps/doc-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/doc-e2e/cypress.json",
            "devServerTarget": "doc:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "doc:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/doc-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["doc"]
    },
    "helpers": {
      "projectType": "library",
      "root": "libs/helpers",
      "sourceRoot": "libs/helpers/src",
      "prefix": "zui",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/helpers"],
          "options": {
            "project": "libs/helpers/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/helpers/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/helpers/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/helpers"],
          "options": {
            "jestConfig": "libs/helpers/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/helpers/src/**/*.ts", "libs/helpers/src/**/*.html"]
          }
        }
      },
      "tags": []
    },
    "next": {
      "projectType": "library",
      "root": "libs/next",
      "sourceRoot": "libs/next/src",
      "prefix": "zui",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/next"],
          "options": {
            "project": "libs/next/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/next/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/next/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/next"],
          "options": {
            "jestConfig": "libs/next/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/next/src/**/*.ts", "libs/next/src/**/*.html"]
          }
        },
        "build-icons": {
          "builder": "./tools/executors/icon-font-builder:build",
          "options": {
            "fontFileName": "ZuiIcons",
            "fontName": "Zui Icons",
            "iconClassName": ".zui-icon",
            "inputPath": "./libs/components/assets/icons",
            "outputPath": "./libs/next/src/styles/icons",
            "outputStyleFile": "icons.less",
            "outputDefinitionsTs": "./libs/next/src/lib/components/icon/icon-definitions.ts",
            "outputDefinitionsVar": "IconDefs"
          }
        },
        "build-icons-16": {
          "builder": "./tools/executors/icon-font-builder:build",
          "options": {
            "fontFileName": "ZuiIcons16",
            "fontName": "Zui Icons 16",
            "iconClassName": ".zui-icon-16",
            "inputPath": "./libs/components/assets/icons-16",
            "outputPath": "./libs/next/src/styles/icons-16",
            "outputStyleFile": "icons-16.less",
            "outputDefinitionsTs": "./libs/next/src/lib/components/icon/icon-16-definitions.ts",
            "outputDefinitionsVar": "Icon16Defs"
          }
        }
      },
      "tags": []
    },
    "schematics": {
      "projectType": "library",
      "root": "libs/schematics",
      "sourceRoot": "libs/schematics/src",
      "prefix": "zyfra",
      "architect": {
        "prebuild": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/schematics"],
          "options": {
            "project": "libs/schematics/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/schematics/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/schematics/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": ["nx prebuild schematics", "nx postbuild schematics"]
          },
          "dependsOn": [
            {
              "target": "build",
              "projects": "dependencies"
            }
          ]
        },
        "postbuild": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              "tsc -p ./libs/schematics/tsconfig.schematics.json",
              "cp ./libs/schematics/src/collection.json ./dist/libs/schematics/src/collection.json",
              "cp ./libs/schematics/src/ng-add/schema.json ./dist/libs/schematics/src/ng-add/schema.json"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/schematics"],
          "options": {
            "jestConfig": "libs/schematics/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/schematics/src/**/*.ts", "libs/schematics/src/**/*.html"]
          }
        }
      },
      "tags": []
    },
    "sdk": {
      "projectType": "application",
      "root": "apps/sdk",
      "sourceRoot": "apps/sdk/src",
      "prefix": "zyfra",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/sdk",
            "index": "apps/sdk/src/index.html",
            "main": "apps/sdk/src/main.ts",
            "polyfills": "apps/sdk/src/polyfills.ts",
            "tsConfig": "apps/sdk/tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": ["apps/sdk/src/favicon.ico", "apps/sdk/src/assets"],
            "extractCss": true,
            "styles": [
              "apps/sdk/src/styles.less",
              "node_modules/primeng/resources/themes/luna-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/@angular/cdk/overlay-prebuilt.css",
              {
                "input": "libs/components/src/styles/theme/default.less",
                "bundleName": "default",
                "inject": false
              },
              "libs/components/src/styles/styles.less",
              "libs/components/src/styles/icons/icons.less",
              "libs/components/src/styles/icons-16/icons-16.less"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/sdk/src/environments/environment.ts",
                  "with": "apps/sdk/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sdk:build:production"
            },
            "development": {
              "browserTarget": "sdk:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sdk:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/sdk/src/**/*.ts", "apps/sdk/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/sdk"],
          "options": {
            "jestConfig": "apps/sdk/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "sdk-e2e": {
      "root": "apps/sdk-e2e",
      "sourceRoot": "apps/sdk-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sdk-e2e/cypress.json",
            "devServerTarget": "sdk:serve:development",
            "tsConfig": "apps/sdk-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sdk:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/sdk-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["sdk"]
    },
    "translate": {
      "projectType": "library",
      "root": "libs/translate",
      "sourceRoot": "libs/translate/src",
      "prefix": "zyfra",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/translate"],
          "options": {
            "project": "libs/translate/ng-package.json"
          },
          "configurations": {
