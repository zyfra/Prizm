              "tsConfig": "libs/translate/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/translate/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/translate"],
          "options": {
            "jestConfig": "libs/translate/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/translate/src/**/*.ts", "libs/translate/src/**/*.html"]
          }
        }
      },
      "tags": []
    }
  }
}
