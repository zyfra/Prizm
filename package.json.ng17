  "license": "MIT",
  "husky": {
  "hooks": {
    "pre-commit": "run-p lint check-branch-name",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  }
},
  "scripts": {
  "check-branch-name": "chmod 755 ./.husky/check_branch_naming.sh && sh ./.husky/check_branch_naming.sh",
    "docs": "nx run components:doc",
    "doc:e2e:pw": "npx nx e2e doc-pw-e2e",
    "format:staged": "pretty-quick --staged",
    "ng": "nx",
    "cypress:open": "cypress open",
    "start": "nx run doc:serve",
    "build": "nx affected:build --all",
    "build:next": "nx run doc:build",
    "build:doc": "nx run doc:build:development --deployUrl=/ --baseHref=/",
    "build:stats": "nx build next --stats-json",
    "analize": "webpack-bundle-analyzer dist/apps/sdk/stats.json",
    "lint:libs": "nx lint schematics",
    "lint:sdk": "nx lint sdk",
    "lint:doc": "nx lint doc",
    "test:doc": "nx test doc",
    "lint:next": "nx lint next",
    "test:next": "nx test next",
    "lint:icons": "nx lint icons",
    "test:icons": "nx test icons",
    "lint:flag-icons": "nx lint flag-icons",
    "test:flag-icons": "nx test flag-icons",
    "doc:e2e": "nx e2e doc-e2e",
    "doc:e2e:watch": "nx e2e doc-e2e --watch",
    "check-next-doc": "npm run lint:doc && npm run test:doc && npm run lint:next && npm run test:next && npm run build:next && npm run doc:e2e",
    "lint": "npm run lint:libs",
    "test": "nx affected:test --all",
    "all": "npm run lint && npm run build",
    "prepare": "husky install"
},
